; GROMACS parameters for single-point energy estimation
continuation	= yes

; Neighborsearching and short-range nonbonded interactions
cutoff-scheme		= Verlet
nstlist			= 1
ns_type			= grid
pbc			= xyz
rlist			= 0.8

; Electrostatics
coulombtype		= PME
rcoulomb		= 0.8

; van der Waals
rvdw			= 0.8

constraints		= all-bonds

ld-seed                  = 1993